{
  "name": "zust-api",
  "version": "0.0.2",
  "description": "A lightweight state management solution based on Zustand",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "homepage": "https://github.com/Smart-Earners-Team/Zustand-API#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Smart-Earners-Team/Zustand-API.git"
  },
  "bugs": {
    "url": "https://github.com/Smart-Earners-Team/Zustand-API/issues"
  },
  "author": "EVMlord <buidl@evmlord.dev>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "dcheck": "npx depcheck",
    "dev:demo": "vite demo",
    "build:demo": "vite build demo",
    "preview:demo": "vite preview demo"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "typesVersions": {
    "*": {
      "core": [
        "dist/types/core/index.d.ts"
      ],
      "core/*": [
        "dist/types/core/*"
      ],
      "types": [
        "dist/types/types/index.d.ts"
      ],
      "types/*": [
        "dist/types/types/*"
      ],
      "utils": [
        "dist/types/utils/index.d.ts"
      ],
      "utils/*": [
        "dist/types/utils/*"
      ]
    }
  },
  "keywords": [
    "state",
    "management",
    "zustand",
    "store",
    "react",
    "state management"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.8.0 <20.0.0",
    "react-dom": ">=16.8.0 <20.0.0",
    "zustand": ">=3.7.2 <6.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "zustand": "^5.0.3"
  }
}
